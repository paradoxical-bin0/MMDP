    <!-- resources section -->

    <section id="resource-section" class="examples-section">
        <div class="container">
            <div class="examples-header flex">
                <h2 class="examples-heading-text">Explore More AI Tools</h2>
            </div>
            <div class="examples-area flex">
                <a class="examples-card" href="#" style="background: linear-gradient(rgb(0, 0, 0, 0.1), rgb(0, 0, 0, 0.8)), url(/assets2/asset\ 35.png); background-size: cover;">
                    <h3 class="card-text">Photo to AI Art Generator</h3>
                </a>
                <a class="examples-card" href="#" style="background: linear-gradient(rgb(0, 0, 0, 0.1), rgb(0, 0, 0, 0.8)), url(/assets2/asset\ 34.png); background-size: cover;">
                    <h3 class="card-text">Text to AI Image Generator</h3>
                </a>
                <a class="examples-card" href="#" style="background: linear-gradient(rgb(0, 0, 0, 0.1), rgb(0, 0, 0, 0.8)), url(/assets2/asset\ 36.png); background-size: cover;">
                    <h3 class="card-text">AI Avatar Generator</h3>
                </a>
                <a class="examples-card" href="#" style="background: linear-gradient(rgb(0, 0, 0, 0.1), rgb(0, 0, 0, 0.8)), url(/assets2/asset\ 37.png); background-size: cover;">
                    <h3 class="card-text">AI Photo Editor</h3>
